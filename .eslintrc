{
  "extends": ["airbnb/base", "prettier"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "no-param-reassign": ["error", { "props": false }],
    "no-unused-vars": "warn",
    "curly": ["error", "all"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
